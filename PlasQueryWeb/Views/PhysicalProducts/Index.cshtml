                for (var i = 1; i < 10; i++) {
                    if ($("#selectparmdivbox").find("div[datatype='" + i + "']").length > 0) {
                        str = "{";//{产品特性=>''高光;阻燃'',0,0} {'产品用途=>''电视外壳''==>'''''}
                        //阻燃等级
                        if (i!=3) {
                            str += $("#selectparmdivbox").find("div[datatype='" + i + "']").attr("bigtitle");
                        }
                        else {
                            str += $("#selectparmdivbox").find("div[datatype='" + i + "']").attr("title");
                        }
                        
                        str += "=>''";
