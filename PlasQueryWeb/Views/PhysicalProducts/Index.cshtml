                            }
                            else {
                                isval = $.trim($(this).attr("title"));
                            }
                            str += isval;
                            if (count < $("#selectparmdivbox").find("div[datatype='" + i + "']").length) {
                                str += ";"
                            }
                        })
                        str += "''==>''''"
                        str += "}"
                        //alert(i+"_"+str);
                    }
                    titlestr += str;
                    str = "";
                }
                // alert(titlestr);
            }
            //2019-2-24{'物理性能=)密度=>1.0,1.5==>''g/cm³'''}
            $.each($("#selectparmdivbox").find("div[datatype='11']"), function (index, item) {
