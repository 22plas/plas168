    }

    .bold {
        /*font-weight: bold;*/
        /*padding: 8px 0;*/
        border: none;
        font-size: 14px;
        color: red !important;
        background: #ffffff;
    }

        .bold td div {
            /*border-bottom: 1px solid #B10820 !important;*/
            /*border-top: 1px solid #B10820 !important;*/
            font-size: 14px;
            height: 40px;
            background: #ffffff;
            color: #B10820;
            /*line-height: 40px;*/
        }

    .pl-10 {
        border: none;
    }
</style>
<div style="width:100%;height:auto;min-height:500px;">
    @*<div style="width:100%;height:auto;min-height:500px;margin:0 auto;color:#333333;float:left;">*@
    @*<div style="width:98%;margin:0 auto;height:auto;border-top: 1px solid #B10820 !important;">*@

    @*</div>*@
    @*</div>*@



    <div style="width:100%;height:auto;min-height:500px;margin:0 auto;color:#333333;">
        <div style="width:98%;margin:0 auto;height:auto;border-top: 1px solid #B10820 !important;">
            @if (@title3 != "" && @title3 != "请选择对比物料")
            {
                foreach (System.Data.DataRow dr in ContrastData.Rows)
                {
                    <div style="width:100%;height:auto;min-height:40px;border-bottom:1px solid #DEDEDE;float:left;">
                        <div class="linheightcss" style="width: 24%;height:auto;min-height:40px;line-height:40px;text-indent:15px;float:left;word-wrap:break-word;">
                            @dr["Attribute"].ToString().Replace(" ", "")@dr["unit"].ToString()@dr["testtype"].ToString()
                        </div>
                        <div class="linheightcss" style="width: 24%;height:auto;min-height:40px;line-height:40px;float:left;text-align:center;margin-left:1%;word-wrap:break-word;">
                        @dr["Attribute1"].ToString().Replace(" ", "")
                        </div>
                        <div class="linheightcss" style="width: 24%;height:auto;min-height:40px;line-height:40px;float:left;text-align:center;margin-left:1%;word-wrap:break-word;">
                        @dr["Attribute2"].ToString().Replace(" ", "")
                        </div>
                        <div class="linheightcss" style="width: 24%;height:auto;min-height:40px;line-height:40px;float:left;text-align:center;margin-left:2%;word-wrap:break-word;">
                        @dr["Attribute3"].ToString().Replace(" ", "")
                        </div>
                    </div>
                }
            }
            else
            {
                foreach (System.Data.DataRow dr in ContrastData.Rows)
                {
                    <div style="width:100%;height:auto;min-height:40px;border-bottom:1px solid #DEDEDE; float:left;">

                        <div class="linheightcss" style="width:33%;height:auto;min-height:40px;float:left;text-align:center;word-wrap:break-word;line-height:35px;">
                            @dr["Attribute"].ToString().Replace(" ", "")@dr["unit"].ToString()@dr["testtype"].ToString()
                        </div>
                        <div class="linheightcss" style="width: 33%;height:auto;min-height:40px;text-align:center;float:left;text-align:center;word-wrap:break-word;line-height:35px;">
                            @dr["Attribute1"].ToString().Replace(" ", "")
                        </div>
                        <div class="linheightcss" style="width: 33%;height:auto;min-height:40px;text-align:center;float:left;text-align:center;word-wrap:break-word;line-height:35px;">
                            @dr["Attribute2"].ToString().Replace(" ", "")
                        </div>
                    </div>
                }
            }
        </div>
        </div>
 </div>
