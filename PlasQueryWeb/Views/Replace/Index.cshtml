<script type="text/javascript">
    $(function () {
        $(".navBox").find(".nav").find("li").eq(3).children("a").addClass("active");
        $("div[name='isHideDiv']").parent().parent().remove();//删除单行数据
    })

 
    function onSearch_Replace_select() {
        var keyword = $.trim($("#search_input_Replace").val());
        if (keyword == "") {
            alert("请输入任意塑料类型、型号、厂家、性能、用途!");
            $("#search_input_Replace").focus();
            return false;
        }
        else {
            window.location.href = "/Product/Index?key=" + encodeURI(encodeURI(keyword));
        }
    }
</script>
