<script type="text/javascript">
    var vm = new Vue({
        el: '#topbar-collapse',
        data: {
            usname: ""//用户名
        }, methods: {
            load: function () {
                $.ajax({
                    type: "get",
                    url: comm.action("UserInfoPartial", "MemberCenter"),
                    data: {},
                    dataType: "json",
                    success: function (data) {
                        console.log(data);
                        if (data.State == "Success") {
                            var rs = data.Result;
                            vm.usname = rs.UserName;
                        }
                        else {
                            vm.usname = "";
                        }
                    }
                });
            }
        }
    });
        $(function () {
            $("#sidebar a[title='@ViewBag.Sidebar']").addClass("addcolor");
            vm.load();
        })

</script>