@{ViewBag.Title = "塑料和弹性体-产品搜索"; }
<link href="~/Content/css/Pagination.css" rel="stylesheet" />
<script src="~/Content/layui/lay/modules/layer.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.pagination.js" type="text/javascript"></script>
@Html.Partial("~/Views/Public/SubPageNav.cshtml")
@{
    Html.RenderAction("PubSearch", "Public");
}

<div class="search-item-box" id="selectTypeList">

</div>

<p style="padding: 10px 20px;" id="selectCheckValue">
    当前已选择条件：<span class="search_condition_wrap">
        <!--<span class="search-select-item">film,PE/PCTFE<span class="fa fa-close" title="删除"></span></span>-->
    </span>
</p>
<div style="padding: 0 20px;box-sizing: border-box;">
    <input type="text" id="Txtkeyword" style="width:100%; height:35px; line-height:35px; border:solid 1px #ddd;" placeholder="请输入任意塑料类型、型号、厂家、性能、用途进行智能搜索" />
</div>
<div style="padding: 0 20px;box-sizing: border-box;" id="Query_btnData">
    <p class="search-btn-screem"><span class="fa fa-search"></span>开始搜索</p>
</div>
<div class="search-content-box">
    <div class="search-content-page">
        共搜索到<span style="color: red;" id="records">0</span>条相关商品信息
    </div>
    <div class="search-content">
        <table>
            <thead>
                <tr>
                    <th>牌号</th>
                    <th>厂商</th>
                    <th>各类</th>
                    <th>用途</th>
                    <th>特性</th>
                </tr>
            </thead>
            <tbody id="tempHtml_list">
                <tr>
                    <td colspan="5" align="center" class='red'>请筛选您需要查询的数据！</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="black2" id="Pagination">
</div>
@Html.Partial("~/Views/Public/Bottom.cshtml")

@Scripts.Render("~/ProductJS/BesPdtJS")

